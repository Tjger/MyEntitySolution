
@{
    ViewBag.Title = "Services";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 
@using EntitySolution.WebResource
@using EntitySolution.Domain.Common
<script>

    var Langguage = '@Session["Culture"].ToString()';
    $(document).ready(function () {
        GetSysPara();

    });

    function GetSysPara() {
        $.ajax({
            url: '@Url.Action("GetSysPara", "AdminPage")',
            contentType: "application/json; charset=utf-8",
            type: 'POST',
            dataType: "json",
            data: JSON.stringify({ "fieldSysPara": '@Var.ConfigServices' }),
            success: function (data) {
                if (Langguage === "vi-VN") {
                    $("#ContactContent").html(data.returnList.Value);
                }
                else {
                    $("#ContactContent").html(data.returnList.DefaultValue);
                }
            }
        });
    };
</script>

<div class="top">
    <div class="left"></div>
    <div class="right"></div>
    <div class="center">
        <h1>@Resource.Services</h1>
    </div>
</div>
<div style="text-align: left;" class="middle" id="ContactContent">

</div>
<div class="bottom">
    <div class="left"></div>
    <div class="right"></div>
    <div class="center"></div>
</div>
<br>

<div class="bottomnd">
    <div class="left"></div>
    <div class="right"></div>
    <div class="center"></div>
</div>
