    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,700italic,400,600,700"
          rel="stylesheet" />

    @Styles.Render("~/Content/bootstrap-3.3.5/css/bootstrap.min.css")
    @Styles.Render("~/Content/bootstrap-3.3.5/css/bootstrap-theme.min.css")
    @Styles.Render("~/Content/FontEnd/css/tanphong.css")
    @Styles.Render("~/Content/FontEnd/css/animations.css")
    @Styles.Render("~/Content/FontEnd/css/buttons.css")
    @Styles.Render("~/Content/FontEnd/css/tabstyles.css")
    
    @*<link rel="stylesheet" href="http://code.jquery.com/ui/1.11.3/themes/smoothness/jquery-ui.css" />*@
    @Styles.Render("~/Content/BackEnd/css/jquery-ui-1.10.3.css")
    @Styles.Render("~/Content/BackEnd/css/font-awesome.min.css")
    
    
    @*<script type="text/javascript" src="js/jquery-1.11.2.min.js"></script>*@
    @Scripts.Render("~/Content/BackEnd/js/jquery-1.11.1.min.js")
    @Scripts.Render("~/Content/bootstrap-3.3.5/js/bootstrap.min.js")
 
    @Scripts.Render("~/Content/FontEnd/js/buttons.js")
 
    @*<script type="text/javascript" src="http://code.jquery.com/ui/1.11.3/jquery-ui.js"></script>*@
    @Scripts.Render("~/Content/BackEnd/js/jquery-ui-1.10.3.min.js")
    @Scripts.Render("~/Content/FontEnd/js/jssor.js")
    @Scripts.Render("~/Content/FontEnd/js/jssor.slider.js")
    @Scripts.Render("~/Content/FontEnd/js/jquery.bxslider.min.js")
 
    <!-- bxSlider CSS file -->
