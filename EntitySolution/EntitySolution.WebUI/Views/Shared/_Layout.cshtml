<body>
    <script type="text/javascript">
        $(document).ready(function () {
           
            $("#btn_close_modal").click(function () {
                playPause();
                alert("dadasda");
            });
        });


        function playPause() {
            if ($("#video1").paused) {
                $("#video1").play();
            }
            else
                $("#video1").pause();
        }

    </script>
    <div class="bg">
        <div class="container box-content">
            <header>
                <div class="top-banner col-md-12 col-lg-12 padd0">
                    <div class="bg-header">
                        <div class="col-md-7 col-lg-7" style="padding-top: 12px;">
                            <a href="#" style="float:left;">
                                <img class="img-responsive" src="@Url.Content("~/Content/FontEnd/images/logotanphong.png")" />
                            </a>
                            <p style="float: left; padding-left: 90px;"><img class="img-responsive" src="@Url.Content("~/Content/FontEnd/images/tencty.png")" /></p>
                        </div>
                        <div class="col-md-5 col-lg-5" style="padding-top: 50px;">
                            <div class="col-md-6 col-lg-6 pull-right">
                                <input type="text" class="form-control" style="font-style: italic;" placeholder="Tìm kiếm..." />
                            </div>
                            <div class="col-md-6 col-lg-6 pull-right" >
                                <select class="form-control" id="cboCategory">
                                    
                                </select>
                            </div>
                        </div>
                        <div class="col-md-12 col-lg-12">
                            <p class="address-top">Ðịa chỉ: Phòng 606, Tòa nhà Indochina Park Tower.Số 4 Nguyễn Đình Chiểu,P. Đa Kao, Q.1,TP.HCM</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-lg-12 padd0">
                    <div class="navbar-wrapper">
                        <nav class="navbar navbar-static-top menu-main">
                            <div class="container">
                                <div class="navbar-header">
                                    <button aria-controls="navbar" aria-expanded="false" data-target="#navbar" data-toggle="collapse" class="navbar-toggle collapsed" type="button">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>

                                </div>
                                <div class="navbar-collapse collapse" id="navbar">
                                    <ul class="nav navbar-nav">
                                        <li><a href="@Url.Action("Index", "Home")">Trang Chủ</a></li>
                                        <li><a href="@Url.Action("About", "Home")">Giới Thiệu</a></li>
                                        <li><a href="@Url.Action("Index", "Home")">Sản Phẩm</a></li>
                                        <li><a href="@Url.Action("Index", "Home")">Tin Công Ty</a></li>
                                        <li><a href="@Url.Action("Contact", "Home")">Liên Hệ</a></li>
                                    </ul>
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>

            </header>
            <div class="main-content">
                <div class="col-md-12 col-lg-12 padd0">
                    <div class="box-newsslider col-md-12 col-lg-12 padd0">
                        <div id="myCarousel" class="carousel slide" data-ride="carousel">
                            <!-- Indicators -->
                            <ol class="carousel-indicators">
                                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                                <li data-target="#myCarousel" data-slide-to="1"></li>
                                <li data-target="#myCarousel" data-slide-to="2"></li>
                                <li data-target="#myCarousel" data-slide-to="3"></li>
                            </ol>
                            <!-- Wrapper for slides -->
                            <div class="carousel-inner" role="listbox">
                                <div class="item active">
                                    <img src="@Url.Content("~/Content/FontEnd/images/sl1.jpg")"  class="img-responsive" />
                                    <div class="carousel-caption">
                                        <h3>
                                            CÔNG TY CỔ PHẦN THƯƠNG MẠI CƠ KHÍ TÂN PHONG
                                        </h3>
                                        <p>
                                            Địa chỉ: Phòng 606, Tòa nhà Indochina Park Tower.
                                            Số 4 Nguyễn Đình Chiểu,P. Đa Kao, Q.1,TP.HCM
                                        </p>
                                    </div>
                                </div>
                                <div class="item">
                                    <img src="@Url.Content("~/Content/FontEnd/images/sl2.png")"   class="img-responsive" />
                                    <div class="carousel-caption">
                                        <h3>
                                            CÔNG TY CỔ PHẦN THƯƠNG MẠI CƠ KHÍ TÂN PHONG
                                        </h3>
                                    </div>
                                </div>
                            </div>
                            <!-- Left and right controls -->
                            <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span><span class="sr-only">
                                    Previous
                                </span>
                            </a><a class="right carousel-control" href="#myCarousel" role="button"
                                   data-slide="next">
                                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true">
                                </span><span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-lg-12 margintop5 padd0">
                    <div class="danhmucsp-menu col-md-3 col-lg-3 padd0">
                        <div class="col-md-12 col-lg-12 padd0">
                            <p class="danhmucsp-menu-title">
                                <i class="fa fa-list" style="padding-right: 10px;"></i>Danh Mục Sản Phẩm
                            </p>
                            <ul class="nav nav-pills nav-stacked" id="lstCategoryPanelLeft">
                                
                            </ul>
                        </div>
                        <div class="col-md-12 col-lg-12 padd0">
                            <p class="danhmucsp-menu-title">
                                <i class="fa fa-cubes" style="padding-right: 10px;"></i>Sản Phẩm Mới
                            </p>
                            <div class="list-newproduct col-md-12 col-lg-12 paddtopbotton10 borderbottom">
                                <ul style="list-style: none;" id="lstItemHotPanelLeft">
                                    
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-12 col-lg-12 padd0">
                            <p class="danhmucsp-menu-title">
                                <i class="fa fa-building" style="padding-right: 10px;"></i>Tin Công Ty
                            </p>
                            <div class="list-newcompany col-md-12 col-lg-12 paddtopbotton10 borderbottom">
                                <ul style="list-style: none;" id="lstNewsPanelLeft">
                                    @*<li>
                                        <a href="#">
                                            <img src="@Url.Content("~/Content/FontEnd/images/newc.png")" class="img-responsive" />
                                            <p class="tittle-small">
                                                SỨ MỆNH: CÔNG TY CP TM CK TÂN PHONG hoạt động với sứ mệnh tạo ra...
                                            </p>
                                            
                                            <span class="link-deatail">Chi tiết</span>
                                        </a>
                                    </li>*@
                                 
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-9 col-lg-9">
                        @RenderBody()
                     
                    </div>
                </div>
            </div>
            <footer>
                <div class="col-md-12 col-lg-12 padd0" style="background: #2642a2;">
                    <div class="col-md-9 col-lg-9 col-md-offset-2 col-lg-offset-2 padd0">
                        <div class="navbar-wrapper">
                            <nav class="navbar navbar-static-top footer-menu">
                                <div id="Div1" class="navbar-collapse collapse">
                                    <ul class="nav navbar-nav">
                                        <li><a href="#">Trang Chủ</a></li>
                                        <li><a href="#">Giới Thiệu</a></li>
                                        <li><a href="#">Sản Phẩm</a></li>
                                        <li><a href="#">Tin Công Ty</a></li>
                                        <li><a href="#">Liên Hệ</a></li>
                                    </ul>
                                </div>
                            </nav>
                        </div>
                    </div>
                </div>
                <div class="bg-footer col-md-12 col-lg-12 padd0">
                    <div class="text-center" style=" padding-bottom: 15px;">
                        <p class="name-company">CÔNG TY CỔ PHẦN THƯƠNG MẠI CƠ KHÍ TÂN PHONG</p>
                        <p class="address-company">Địa chỉ: Phòng 606, Tòa nhà Indochina Park Tower.</p>
                        <p class="address-company">Số 4 Nguyễn Đình Chiểu,P. Đa Kao, Q.1,TP.HCM</p>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</body>
</html>
